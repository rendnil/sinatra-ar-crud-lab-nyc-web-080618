<h1>Edit post</h1>

<form action="/posts/<%=@post.id%>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
      <p>Name:<input type="text" value = "<%=@post.name%>" name="name"></p>
      <p>Content:<input type="text" value = "<%=@post.content%>" name="content"></p>
  <input type="submit" id = "submit">
</form>
